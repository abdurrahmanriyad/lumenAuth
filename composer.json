{
  "name": "abdurrahmanriyad/lumenauth",
  "description": "A package to implement easy authentication in lumen",
  "keywords": ["auth", "lumenauth", "authentication"],
  "type": "library",
  "require": {
    "php": ">=5.5.9",
    "firebase/php-jwt": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "Abdurrahmanriyad\\LumenAuth\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Abdur Rahman",
      "email": "me.riyad@outlook.com"
    }
  ],
  "minimum-stability": "dev",

  "extra": {
      "laravel": {
          "providers": [
              "Abdurrahmanriyad\\LumenAuth\\LumenAuthServiceProvider"
          ],
          "aliases": {
              "Permit": "Abdurrahmanriyad\\LumenAuth\\Facades\\LumenAuth"
          }
      }
  }
}